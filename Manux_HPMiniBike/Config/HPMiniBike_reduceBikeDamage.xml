<configs>
	<config name="vehicles">
		<!-- MANUX HP MiniBike: optional changes to reduce damage to the minibike -->
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='chassis']/property[@name='part_damage_reduction']/@value">1</set>
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='handlebars']/property[@name='part_damage_reduction']/@value">1</set>
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='wheel1']/property[@name='part_damage_reduction']/@value">1</set>
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='driver']/property[@name='part_damage_reduction']/@value">1</set>
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='engine']/property[@name='part_damage_reduction']/@value">1</set>
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='storage']/property[@name='part_damage_reduction']/@value">1</set>
		<set xpath="/vehicles/vehicle[@name='minibike']/property[@class='lock']/property[@name='part_damage_reduction']/@value">1</set>
	</config>
</configs>