		</append>
		<append xpath="/traders/trader_item_groups/trader_item_group[@name='preparedFood']">
			<!-- MANUX Food Recipes -->
			<item name="baconEggSandwich"/>
			<item name="padThai"/>
			<item name="sheperdsPie"/>
			<item name="burgerAndFries"/>
			<item name="Omelet"/>
			<item name="scrambledEggs"/>
			<item name="steakSandwich"/>
		</append>
	</config>
</configs>