<configs>
	<config name="sounds">	
		<append xpath="/Sounds">
			<!--MANUX Custom Vehicles: JaxTeller718's Road Hog Sounds --> 
			<SoundDataNode name="roadHog_accel_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<Noise ID="3" range="14" volume="16" time="3" muffled_when_crouched="0.65" heat_map_strength="0.05" heat_map_time="60"/>
				<AudioClip ClipName="#JaxTeller718RoadHogSounds2?Chopper Accel"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="2"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_decel_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<AudioClip ClipName="#JaxTeller718RoadHogSounds2?Chopper Decel"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="2"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_empty_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<Noise ID="1" range="4" volume="7" time="5" muffled_when_crouched="0.5"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_empty_1"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_empty_2"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="10"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_horn"> <AudioSource name="Sounds/AudioSource_SmallHorn"/>
				<Noise ID="4" range="25" volume="20" time="2" heat_map_strength="0.2" heat_map_time="90"/>
				<AudioClip ClipName="#JaxTeller718RoadHogHorns?Horn Cucaracha"/>
				<AudioClip ClipName="#JaxTeller718RoadHogHorns?Horn Godfather"/>
				<AudioClip ClipName="#JaxTeller718RoadHogHorns?Horn Hazard"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="1"/> <NoiseScale name="1"/> <MaxVoices name="3"/> <MaxEntities name="6"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_idle_lp_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<AudioClip ClipName="#JaxTeller718RoadHogSounds2?Chopper Idle" Loop="true"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="10"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_impact_"> <AudioSource name="Sounds/AudioSource_Impact"/>
				<Noise ID="2" range="7" volume="11" time="4" muffled_when_crouched="0.5"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_impact_1"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_impact_2"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_impact_3"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_impact_4"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="2"/> <MaxRepeatRate name="1.2"/> </SoundDataNode>

			<SoundDataNode name="roadHog_run_lp_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<AudioClip ClipName="#JaxTeller718RoadHogSounds2?Chopper Run" Loop="true"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="10"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_shutoff_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<AudioClip ClipName="#JaxTeller718RoadHogSounds2?Chopper Decel"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="10"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_start_"> <AudioSource name="Sounds/AudioSource_Vehicle"/>
				<Noise ID="2" range="7" volume="11" time="4" muffled_when_crouched="0.5"/>
				<AudioClip ClipName="#JaxTeller718RoadHogSounds2?Chopper Start"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="10"/> <MaxRepeatRate name="0.01"/> </SoundDataNode>

			<SoundDataNode name="roadHog_suspension_"> <AudioSource name="Sounds/AudioSource_Impact"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_suspension_1"/>
				<AudioClip ClipName="Sounds/Vehicles/Minibike/minibike_suspension_2"/>
				<LocalCrouchVolumeScale name="1.0"/> <CrouchNoiseScale name="0.5"/> <NoiseScale name="1"/> <MaxVoices name="3"/> <MaxRepeatRate name="1.0"/> </SoundDataNode>
		</append>
	</config>
</configs>